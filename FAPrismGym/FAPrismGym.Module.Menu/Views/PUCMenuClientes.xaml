<UserControl x:Class="FAPrismGym.Module.Menu.Views.PUCMenuClientes"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <StackPanel Orientation="Horizontal" >
        <Button Content="Nuevo" Command="{Binding NuevoClienteCommand}" Style="{DynamicResource BSMainUC}"/>
        <Button Content="Modificar" Command="{Binding ModificarClienteCommand}" Style="{DynamicResource BSMainUC}"/>
    </StackPanel>
</UserControl>
