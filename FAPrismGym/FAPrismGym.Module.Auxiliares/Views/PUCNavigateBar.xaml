<UserControl x:Class="FAPrismGym.Module.Auxiliares.Views.PUCNavigateBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True"
             Height="50" Width="Auto" Margin="10">
    <StackPanel Orientation="Horizontal" >
        <Button Content="Back" Padding="5" Margin="5" Style="{DynamicResource BSPrincipal}" Width="70" Command="{Binding BackCommand}"/>
        <Button Content="Home" Padding="5" Margin="0,5,0,5" Style="{DynamicResource BSPrincipal}" Width="70" Command="{Binding HomeCommand}"/>
        <Button Content="Next" Padding="5" Margin="5,5,0,5" Style="{DynamicResource BSPrincipal}" Width="70" Command="{Binding NextCommand}"/>
        <Button Content="Cancel" Padding="5" Margin="5" Style="{DynamicResource BSPrincipal}" Width="70" Command="{Binding CancelCommand}"/>
    </StackPanel>
</UserControl>
