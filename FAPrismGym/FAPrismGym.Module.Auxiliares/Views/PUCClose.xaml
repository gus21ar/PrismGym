<UserControl x:Class="FAPrismGym.Module.Auxiliares.Views.PUCClose"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True"
             Height="75" Width="150">
    <StackPanel Orientation="Horizontal">
        <Button Content="-" Margin="5" Height="65" Width="40" Style="{DynamicResource BSPrincipal}" Command="{Binding MinimizeCommand}"/>
        <Button Content="0" Margin="5" Height="65" Width="40" Style="{DynamicResource BSPrincipal}" Command="{Binding MinimizeCommand}"/>
        <Button Content="X" Margin="5" Height="65" Width="40" Style="{DynamicResource BSPrincipal}" Command="{Binding MinimizeCommand}"/>
    </StackPanel>
</UserControl>
